<%- include('./partials/headerLogueado') %>


			<div class="container products-wrapper">
				<div class="row">
					<div class="col-12">
						<h2 class="products-title">Resultado de Busqueda</h2>
					</div>
					<% for(let i = 0 ; i=resultados.length; i++) {  %> 
		
					<!-- Show the "ultimos" products -->
					
					<div class="col-12 col-sm-6 col-lg-3">
						<section class="product-box">
							<a href="/producto/<%=resultados[i].id %> ">
								<figure class="product-box_image">
									<img class= "img-producto" src="<%=resultados[i].imagen %> " alt="<%=resultados[i].nombre%>">
								</figure>
								<article class="product-box_data">
									<h2><%=resultados[i].nombre%></h2>
									<p><%=resultados[i].descripcion%></p>
									
								</article>
							</a>
						</section>
					</div>
					   <%  } %> 
				</div> 
			</div>


<%- include('./partials/footer') %> 	